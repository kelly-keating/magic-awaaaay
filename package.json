{
  "name": "magic-awaaaay",
  "version": "1.0.0",
  "description": "Magic card collection app",
  "main": "server/index.ts",
  "repository": "https://github.com/kelly-keating/magic-awaaaay.git",
  "author": "Kelly Keating <keating.kel@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "run-s build:client build:server",
    "build:client": "vite build",
    "build:server": "tsc -p server/tsconfig.json",
    "start": "node server/index.js",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "dev": "run-p dev:client dev:server",
    "dev:client": "vite",
    "dev:server": "nodemon server/index.ts",
    "knex": "knex --knexfile ./server/db/knexfile.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier --write .",
    "reseed": "run-s seed:sets seed:cards seed:money",
    "seed:sets": "ts-node data/processSets",
    "seed:cards": "ts-node data/processCards",
    "seed:money": "npm run knex seed:run"
  },
  "eslintConfig": {
    "extends": "@devacademy/eslint-config/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.0",
    "@chakra-ui/icons": "^2.1.0",
    "@chakra-ui/react": "^2.8.0",
    "@devacademy/eslint-config": "^1.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "dotenv": "^16.3.1",
    "express": "^4.17.2",
    "express-jwt": "^8.4.1",
    "express-oauth2-jwt-bearer": "^1.5.0",
    "framer-motion": "^10.14.0",
    "jsonwebtoken": "^9.0.1",
    "jwks-rsa": "^3.0.1",
    "knex": "^2.3.0",
    "sqlite3": "^5.1.2",
    "superagent": "7.1.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@devacademy/eslint-config": "^1.8.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4",
    "@testing-library/user-event": "^14.4.3",
    "@types/express": "^4.17.14",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@types/supertest": "^2.0.12",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.30.1",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nock": "^13.3.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "react": "^18.0.2",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "sass": "^1.62.1",
    "supertest": "^6.2.2",
    "typescript": "~4.8.4",
    "vite": "^4.3.3",
    "vitest": "^0.30.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "browserslist": "> 2%, not dead"
}
